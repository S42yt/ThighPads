{
    "name": "TypeScript",
    "author": "ThighPads",
    "version": "1.0",
    "language": "typescript",
    "tags": ["ts", "typescript"],
    "tokenColors": {
      "keyword": "#CBA6F7",
      "string": "#FAB387",
      "number": "#F9E2AF",
      "comment": "#585B70",
      "function": "#89B4FA",
      "type": "#89DCEB",
      "variable": "#B4BEFE",
      "operator": "#F5C2E7"
    },
    "rules": [
      {
        "pattern": "\\b(const|let|var|function|return|if|else|for|while|switch|case|break|continue|class|extends|implements|import|export|from|try|catch|finally|interface|type|enum|namespace|module|declare|as|typeof|of|in|instanceof|new|this|async|await|public|private|protected|readonly|static|get|set)\\b",
        "token": "keyword"
      },
      {
        "pattern": "\"(?:\\\\\"|[^\"])*?\"|'(?:\\\\'|[^'])*?'|`(?:\\\\`|[^`])*?`",
        "token": "string"
      },
      {
        "pattern": "\\b\\d+(\\.\\d+)?([eE][+-]?\\d+)?\\b|0x[a-fA-F\\d]+\\b|0b[01]+\\b",
        "token": "number"
      },
      {
        "pattern": "\\/\\/.*$|\\/\\*[\\s\\S]*?\\*\\/",
        "token": "comment"
      },
      {
        "pattern": "\\b[A-Za-z_][A-Za-z0-9_]*(?=\\s*\\()",
        "token": "function"
      },
      {
        "pattern": "\\b(Array|Boolean|Date|Error|Function|Map|Math|Number|Object|Promise|RegExp|Set|String|Symbol|any|unknown|never|void)\\b|\\b[A-Z][A-Za-z0-9_]*\\b",
        "token": "type"
      },
      {
        "pattern": "\\b[a-z][A-Za-z0-9_]*\\b",
        "token": "variable"
      },
      {
        "pattern": "\\+|\\-|\\*|\\/|%|=|==|===|!=|!==|>|>=|<|<=|&&|\\|\\||\\!|\\?|:|\\??|\\.\\.\\.|\\.|\\/=|\\*=|%=|\\+=|\\-=|<<=|>>=|>>>=|&=|\\^=|\\|=|=>|\\+\\+|\\-\\-|\\?\\.|\\*\\*",
        "token": "operator"
      }
    ]
  }